        "# This question is being asked in hacker rank \n",
        "# Guest welcome mat\n",
        "\n",
        "N = int(input())\n",
        "M = int(input())\n",
        "for i in range(N):\n",
        "  pattern = \".|.\"\n",
        "  if i < (N-1)/2:\n",
        "    print((pattern * (2*i+1)).center(M, \"-\"))\n",
        "  elif i == (N-1)/2:\n",
        "    print(\"WELCOME\".center(M, \"-\"))\n",
        "  else:\n",
        "    print((pattern * (2*(N-1-i)+1)).center(M, \"-\"))"
      ]
